{
  "compilerOptions": {
    "incremental": true,
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": false,
    "outDir": "dist"
  },
  "include": [
    "src/**/*"
  ],
}
